<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dance="clr-namespace:Dance.Wpf">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Dance.Wpf;component/Share/Xaml/Design/MouseOver/DanceMouseOver.xaml"></ResourceDictionary>
        <ResourceDictionary Source="/Dance.Wpf;component/Share/Xaml/Design/Separator/DanceSeparator.xaml"></ResourceDictionary>
        <ResourceDictionary Source="/Dance.Wpf;component/Share/Xaml/Design/Shuttle/DanceShuttle.xaml"></ResourceDictionary>
        <ResourceDictionary Source="/Dance.Wpf;component/Share/Xaml/Design/Svg/DanceSvgImage.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="dance:DanceDashedLine">
        <Setter Property="Stroke" Value="{DynamicResource ResourceKey={x:Static Member=dance:DanceResourceKey.BORDER_BRUSH}}"></Setter>
    </Style>

    <Style TargetType="dance:DanceDashedRectangle">
        <Setter Property="Stroke" Value="{DynamicResource ResourceKey={x:Static Member=dance:DanceResourceKey.BORDER_BRUSH}}"></Setter>
    </Style>

</ResourceDictionary>