<Window x:Class="Dance.WpfTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dance="http://schemas.dance.com/wpf"
        xmlns:local="clr-namespace:Dance.WpfTest" 
        xmlns:dock="https://github.com/Dirkster99/AvalonDock"
        xmlns:toolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d" Background="White" x:Name="uc"
        Title="MainWindow" Height="800" Width="1400">

    <Window.Resources>
        <ResourceDictionary Source="pack://application:,,,/Dance.Wpf;component/Themes/ToolKit/PropertyGrid.xaml"></ResourceDictionary>
    </Window.Resources>

    <Grid Background="Transparent">
        <toolkit:PropertyGrid x:Name="propertyGrid" Margin="20" Background="White">

        </toolkit:PropertyGrid>
    </Grid>
</Window>
